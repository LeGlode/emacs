(require 'autothemer)
(autothemer-deftheme 
 sombre "Th√®me sombre"
 ((((class color) (min-colors #xFFFFFF)))
  (sombre-noir "#000000")
  (sombre-blanc "#eef4d4")
  (sombre-vert "#1a871a")
  (sombre-violet "#8b31d6")
  (sombre-orange "#ff7a21"))

 ((default (:foreground sombre-vert :background sombre-noir))
  (cursor (:background sombre-orange))))
(provide-theme 'sombre)
